<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>

    <package name="goodcount" namespace="/" extends="myIntercepter2">
        
        <action name="goodcountAction_*" class="goodcountAction" method="{1}">
			
			<result name="getAll success">/JSP/BackStage/GoodCount/goodcount_list.jsp</result>
			<result name="getAll">/JSP/BackStage/GoodCount/goodcount_list.jsp</result>
			<result name="orderGoods">/JSP/BackStage/GoodCount/goodcount_detail.jsp</result>
			<result name="error">/JSP/BackStage/error.jsp</result>
		</action>
    </package>
    

</struts>
